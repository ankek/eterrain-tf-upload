# Docker Compose Environment Configuration
# Copy this file to .env and adjust values as needed

# Host port mapping (host:container)
# The port on your host machine that will map to container port 7777
HOST_PORT=7777

# Storage Configuration
# Options: "csv" for data upload service, "memory" for state backend
STORAGE_TYPE=csv

# TLS Configuration
# Set to true to enable HTTPS (requires certificate files)
ENABLE_TLS=false
TLS_CERT_FILE=
TLS_KEY_FILE=

# Note: Other configurations like HOST, PORT, and STORAGE_PATH
# are set in docker-compose.yml and should not be changed here
# unless you modify the docker-compose.yml accordingly
